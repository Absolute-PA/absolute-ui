{
  "name": "nodeweb-master-ui",
  "version": "0.1.39",
  "private": true,
  "scripts": {
    "dev": "next dev ",
    "dev-https": "NEXT_PUBLIC_HTTPS=true NODE_OPTIONS='-r @newrelic/next' HTTPS=true node server.js",
    "start-https": "NEXT_PUBLIC_HTTPS=true NODE_ENV=production NODE_OPTIONS='-r @newrelic/next' HTTPS=true node server.js",
    "build": "NEXT_PUBLIC_HTTPS=true next build",
    "start": "NODE_OPTIONS='-r @newrelic/next' next start",
    "lint": "next lint",
    "compile": "tsc -b tsconfig.json --incremental",
    "docker:build": "docker build -t nodeweb-master-ui .",
    "deploy": "./scripts/prepareBuild.sh",
    "deployUat": "./scripts/prepareBuildUat.sh",
    "docker:save": "docker save -o ./out/nodeweb-master-ui.tar nodeweb-master-ui"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/serialize": "^1.1.1",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-common-types": "^6.4.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@newrelic/next": "^0.7.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/newrelic": "^9.14.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/react-mic": "^12.4.3",
    "antd": "5.6.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.3.6",
    "dayjs": "^1.11.7",
    "dotenv": "^16.4.5",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "firebase": "^9.20.0",
    "js-cookie": "^3.0.4",
    "lodash": "^4.17.21",
    "newrelic": "^11.1.0",
    "next": "13.3.0",
    "next-redux-wrapper": "^8.1.0",
    "next-runtime-env": "1.x",
    "peerjs": "^1.4.7",
    "postcss": "8.4.22",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-multi-date-picker": "^4.0.0",
    "react-redux": "^8.0.5",
    "redux-thunk": "^2.4.2",
    "socket.io-client": "^4.6.1",
    "tailwindcss": "3.3.1",
